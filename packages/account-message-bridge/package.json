{
  "name": "@sophon-labs/account-message-bridge",
  "version": "0.0.1",
  "description": "Message bridge to handle communication between web and mobile",
  "author": "Sophon",
  "main": "dist/index.cjs",
  "module": "dist/index.js",
  "types": "dist/index.d.ts",
  "exports": {
    ".": {
      "import": {
